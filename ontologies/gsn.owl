        <rdfs:domain rdf:resource="http://www.semanticweb.org/momcilovic/ontologies/2024/1/gsn#Goal"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/momcilovic/ontologies/2024/1/gsn#Strategy"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/momcilovic/ontologies/2024/1/gsn#Assumption"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/momcilovic/ontologies/2024/1/gsn#Context"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/momcilovic/ontologies/2024/1/gsn#Justification"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/momcilovic/ontologies/2024/1/gsn#Module"/>
        <coreOrExtension xml:lang="en">Core GSN</coreOrExtension>
        <rdfs:label xml:lang="en">in context of</rdfs:label>
        <skos:definition xml:lang="en">InContextOf, rendered as a line with a hollow arrowhead, declares a contextual relationship.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/momcilovic/ontologies/2024/1/gsn#relatedTo -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/momcilovic/ontologies/2024/1/gsn#relatedTo">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/momcilovic/ontologies/2024/1/gsn#Pattern"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/momcilovic/ontologies/2024/1/gsn#Pattern"/>
        <coreOrExtension xml:lang="en">Argument Pattern Extension</coreOrExtension>
        <rdfs:label xml:lang="en">related to</rdfs:label>
        <skos:definition xml:lang="en">This section can be used to reference patterns that are related e.g. addressing the same intent in a different context.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/momcilovic/ontologies/2024/1/gsn#substitutedBy -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/momcilovic/ontologies/2024/1/gsn#substitutedBy">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/momcilovic/ontologies/2024/1/gsn#Justification"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/momcilovic/ontologies/2024/1/gsn#Goal"/>
        <coreOrExtension>Modular Extension</coreOrExtension>
        <rdfs:label xml:lang="en">substituted by</rdfs:label>
        <skos:definition xml:lang="en">There may be occasions when a goal or strategy requires fuller justification than can be provided within the confines of a GSN justification element. In such cases, an away goal can be substituted for the justification.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/momcilovic/ontologies/2024/1/gsn#supportedBy -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/momcilovic/ontologies/2024/1/gsn#supportedBy">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/momcilovic/ontologies/2024/1/gsn#Goal"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/momcilovic/ontologies/2024/1/gsn#Strategy"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/momcilovic/ontologies/2024/1/gsn#Goal"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/momcilovic/ontologies/2024/1/gsn#Module"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/momcilovic/ontologies/2024/1/gsn#Solution"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/momcilovic/ontologies/2024/1/gsn#Strategy"/>
        <coreOrExtension xml:lang="en">Core GSN</coreOrExtension>
        <rdfs:label xml:lang="en">supported by</rdfs:label>
        <skos:definition xml:lang="en">SupportedBy, rendered as a line with a solid arrowhead, allows support relationships between elements to be documented.</skos:definition>
    </owl:ObjectProperty>
    


    <!-- http://www.w3.org/1999/02/22-rdf-syntax-ns#object -->

    <owl:ObjectProperty rdf:about="http://www.w3.org/1999/02/22-rdf-syntax-ns#object">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/momcilovic/ontologies/2024/1/gsn#RelationshipWithConfidence"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/momcilovic/ontologies/2024/1/gsn#Context"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/momcilovic/ontologies/2024/1/gsn#Solution"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/momcilovic/ontologies/2024/1/gsn#Strategy"/>
    </owl:ObjectProperty>
    


    <!-- http://www.w3.org/1999/02/22-rdf-syntax-ns#predicate -->

    <owl:ObjectProperty rdf:about="http://www.w3.org/1999/02/22-rdf-syntax-ns#predicate">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/momcilovic/ontologies/2024/1/gsn#RelationshipWithConfidence"/>
    </owl:ObjectProperty>
    


    <!-- http://www.w3.org/1999/02/22-rdf-syntax-ns#subject -->

    <owl:ObjectProperty rdf:about="http://www.w3.org/1999/02/22-rdf-syntax-ns#subject">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/momcilovic/ontologies/2024/1/gsn#RelationshipWithConfidence"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/momcilovic/ontologies/2024/1/gsn#Goal"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/momcilovic/ontologies/2024/1/gsn#Strategy"/>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.semanticweb.org/momcilovic/ontologies/2024/1/gsn#argumentType -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/momcilovic/ontologies/2024/1/gsn#argumentType">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:oneOf>
                    <rdf:Description>
                        <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                        <rdf:first>confidence</rdf:first>
                        <rdf:rest>
                            <rdf:Description>
                                <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                                <rdf:first>risk</rdf:first>
                                <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                            </rdf:Description>
                        </rdf:rest>
                    </rdf:Description>
                </owl:oneOf>
            </rdfs:Datatype>
        </rdfs:range>
        <coreOrExtension>Confidence Argument Extension</coreOrExtension>
        <rdfs:label xml:lang="en">argument type</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/momcilovic/ontologies/2024/1/gsn#assuranceClaimPoint -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/momcilovic/ontologies/2024/1/gsn#assuranceClaimPoint">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/momcilovic/ontologies/2024/1/gsn#Context"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/momcilovic/ontologies/2024/1/gsn#RelationshipWithConfidence"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/momcilovic/ontologies/2024/1/gsn#Solution"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <coreOrExtension>Confidence Argument Extension</coreOrExtension>
        <renderedAs>solid square</renderedAs>
        <rdfs:label xml:lang="en">assurance claim point</rdfs:label>
        <skos:definition xml:lang="en">An Assurance Claim Point (ACP) can be used in GSN to indicate that a confidence argument is associated with an assertion in a risk argument.</skos:definition>
    </owl:DatatypeProperty>
    <owl:Axiom>
        <owl:annotatedSource rdf:resource="http://www.semanticweb.org/momcilovic/ontologies/2024/1/gsn#assuranceClaimPoint"/>
        <owl:annotatedProperty rdf:resource="http://www.w3.org/2000/01/rdf-schema#domain"/>
        <owl:annotatedTarget rdf:resource="http://www.semanticweb.org/momcilovic/ontologies/2024/1/gsn#Context"/>
        <coreOrExtension>Argument Pattern Extension</coreOrExtension>
    </owl:Axiom>
    <owl:Axiom>
        <owl:annotatedSource rdf:resource="http://www.semanticweb.org/momcilovic/ontologies/2024/1/gsn#assuranceClaimPoint"/>
        <owl:annotatedProperty rdf:resource="http://www.w3.org/2000/01/rdf-schema#domain"/>
        <owl:annotatedTarget rdf:resource="http://www.semanticweb.org/momcilovic/ontologies/2024/1/gsn#RelationshipWithConfidence"/>
        <coreOrExtension>Argument Pattern Extension</coreOrExtension>
    </owl:Axiom>
    <owl:Axiom>
        <owl:annotatedSource rdf:resource="http://www.semanticweb.org/momcilovic/ontologies/2024/1/gsn#assuranceClaimPoint"/>
        <owl:annotatedProperty rdf:resource="http://www.w3.org/2000/01/rdf-schema#domain"/>
        <owl:annotatedTarget rdf:resource="http://www.semanticweb.org/momcilovic/ontologies/2024/1/gsn#Solution"/>
        <coreOrExtension>Argument Pattern Extension</coreOrExtension>
    </owl:Axiom>
    


    <!-- http://www.semanticweb.org/momcilovic/ontologies/2024/1/gsn#awayReference -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/momcilovic/ontologies/2024/1/gsn#awayReference">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/momcilovic/ontologies/2024/1/gsn#Assumption"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/momcilovic/ontologies/2024/1/gsn#Context"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/momcilovic/ontologies/2024/1/gsn#Goal"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/momcilovic/ontologies/2024/1/gsn#Justification"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/momcilovic/ontologies/2024/1/gsn#Solution"/>
        <coreOrExtension>Modular Extension</coreOrExtension>
        <renderedAs>bisecting line in the lower half</renderedAs>
        <rdfs:label xml:lang="en">away reference</rdfs:label>
        <skos:definition xml:lang="en">An away element reference repeats a claim or evidence presented in another argument module.</skos:definition>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/momcilovic/ontologies/2024/1/gsn#choice -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/momcilovic/ontologies/2024/1/gsn#choice">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/momcilovic/ontologies/2024/1/gsn#RelationshipWithConfidence"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <coreOrExtension xml:lang="en">Argument Pattern Extension</coreOrExtension>
        <renderedAs>solid diamond</renderedAs>
        <rdfs:label xml:lang="en">choice</rdfs:label>
        <skos:definition xml:lang="en">A GSN choice can be used to denote possible alternatives in satisfying a relationship.</skos:definition>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/momcilovic/ontologies/2024/1/gsn#confidence -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/momcilovic/ontologies/2024/1/gsn#confidence">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/momcilovic/ontologies/2024/1/gsn#Context"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/momcilovic/ontologies/2024/1/gsn#RelationshipWithConfidence"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/momcilovic/ontologies/2024/1/gsn#Solution"/>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:onDatatype rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
                <owl:withRestrictions rdf:parseType="Collection">
                    <rdf:Description>
                        <xsd:minInclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#decimal">0.0</xsd:minInclusive>
                    </rdf:Description>
                    <rdf:Description>
                        <xsd:maxInclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#decimal">1.0</xsd:maxInclusive>
                    </rdf:Description>
                </owl:withRestrictions>
            </rdfs:Datatype>
        </rdfs:range>
        <coreOrExtension>Confidence Argument Extension</coreOrExtension>
        <rdfs:label xml:lang="en">confidence</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/momcilovic/ontologies/2024/1/gsn#contract -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/momcilovic/ontologies/2024/1/gsn#contract">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/momcilovic/ontologies/2024/1/gsn#Module"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <owl:propertyDisjointWith rdf:resource="http://www.semanticweb.org/momcilovic/ontologies/2024/1/gsn#undeveloped"/>
        <coreOrExtension>Modular Extension</coreOrExtension>
        <rdfs:label xml:lang="en">contract</rdfs:label>
        <skos:definition xml:lang="en">This decorator denotes that support for the claim presented by the attached goal is intended to be provided from an argument in another module linked by an as-yet-undisclosed contract.</skos:definition>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/momcilovic/ontologies/2024/1/gsn#contractReference -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/momcilovic/ontologies/2024/1/gsn#contractReference">
        <coreOrExtension>Modular Extension</coreOrExtension>
        <renderedAs>rectangle with a two smaller rectangles adjoining at the top left and bottom right</renderedAs>
        <rdfs:label xml:lang="en">contract reference</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/momcilovic/ontologies/2024/1/gsn#defeated -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/momcilovic/ontologies/2024/1/gsn#defeated">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/momcilovic/ontologies/2024/1/gsn#Assumption"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/momcilovic/ontologies/2024/1/gsn#Context"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/momcilovic/ontologies/2024/1/gsn#Goal"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/momcilovic/ontologies/2024/1/gsn#Justification"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/momcilovic/ontologies/2024/1/gsn#Relationship"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/momcilovic/ontologies/2024/1/gsn#Solution"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/momcilovic/ontologies/2024/1/gsn#Strategy"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <coreOrExtension>Dialectic Extension</coreOrExtension>
        <renderedAs>cross (‘X’) superimposed on a GSN element or relationship</renderedAs>
        <rdfs:label xml:lang="en">defeated</rdfs:label>
        <skos:definition xml:lang="en">This indicates that the element is defeated. The Defeated decorator can be applied to any of the GSN elements.</skos:definition>
    </owl:DatatypeProperty>
    <owl:Axiom>
        <owl:annotatedSource rdf:resource="http://www.semanticweb.org/momcilovic/ontologies/2024/1/gsn#defeated"/>
        <owl:annotatedProperty rdf:resource="http://www.w3.org/2000/01/rdf-schema#domain"/>
        <owl:annotatedTarget rdf:resource="http://www.semanticweb.org/momcilovic/ontologies/2024/1/gsn#Relationship"/>
        <skos:definition xml:lang="en">Defeated Relationship indicates that the relationship is defeated. The Defeated decorator can be applied to any of the GSN relationships.</skos:definition>
    </owl:Axiom>
    


    <!-- http://www.semanticweb.org/momcilovic/ontologies/2024/1/gsn#final -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/momcilovic/ontologies/2024/1/gsn#final">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <coreOrExtension xml:lang="en">Argument Pattern Extension</coreOrExtension>
        <rdfs:label xml:lang="en">final</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/momcilovic/ontologies/2024/1/gsn#inDoubt -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/momcilovic/ontologies/2024/1/gsn#inDoubt">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/momcilovic/ontologies/2024/1/gsn#Assumption"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/momcilovic/ontologies/2024/1/gsn#Context"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/momcilovic/ontologies/2024/1/gsn#Goal"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/momcilovic/ontologies/2024/1/gsn#Justification"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/momcilovic/ontologies/2024/1/gsn#Relationship"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/momcilovic/ontologies/2024/1/gsn#Solution"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/momcilovic/ontologies/2024/1/gsn#Strategy"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <coreOrExtension>Dialectic Extension</coreOrExtension>
        <rdfs:label xml:lang="en">in doubt</rdfs:label>
        <skos:definition xml:lang="en">The dialectic challenge within this structure asserts that if the claim presented in Goal CG1 is true then this is sufficient to establish that the claim in Goal G1 in the original structure is in doubt.</skos:definition>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/momcilovic/ontologies/2024/1/gsn#instantiationDataReference -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/momcilovic/ontologies/2024/1/gsn#instantiationDataReference">
        <coreOrExtension>Argument Pattern Extension</coreOrExtension>
        <renderedAs xml:lang="en">hollow triangle</renderedAs>
        <rdfs:label xml:lang="en">instantiation data reference</rdfs:label>
        <skos:definition xml:lang="en">This symbol indicates that the GSN argument below the attached element is to be instantiated as a template argument. It provides a reference to the information used to instantiate the template argument.</skos:definition>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/momcilovic/ontologies/2024/1/gsn#maxCardinality -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/momcilovic/ontologies/2024/1/gsn#maxCardinality">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/momcilovic/ontologies/2024/1/gsn#Relationship"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"/>
        <coreOrExtension xml:lang="en">Argument Pattern Extension</coreOrExtension>
        <rdfs:label xml:lang="en">maximum cardinality</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/momcilovic/ontologies/2024/1/gsn#minCardinality -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/momcilovic/ontologies/2024/1/gsn#minCardinality">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/momcilovic/ontologies/2024/1/gsn#Relationship"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"/>
        <coreOrExtension xml:lang="en">Argument Pattern Extension</coreOrExtension>
        <rdfs:label xml:lang="en">minimum cardinality</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/momcilovic/ontologies/2024/1/gsn#moduleReference -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/momcilovic/ontologies/2024/1/gsn#moduleReference">
        <coreOrExtension>Modular Extension</coreOrExtension>
        <renderedAs xml:lang="en">rectangle with a second smaller rectangle adjoining at the top left</renderedAs>
        <rdfs:label xml:lang="en">module reference</rdfs:label>
        <skos:definition xml:lang="en">A module reference presents a reference to a module containing an argument. Note that a module reference points to the totality of the argument contained in the referenced argument module, rather than just to an individual claim.</skos:definition>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/momcilovic/ontologies/2024/1/gsn#multiple -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/momcilovic/ontologies/2024/1/gsn#multiple">
        <coreOrExtension xml:lang="en">Argument Pattern Extension</coreOrExtension>
        <renderedAs>solid ball</renderedAs>
        <rdfs:label xml:lang="en">multiple</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/momcilovic/ontologies/2024/1/gsn#optional -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/momcilovic/ontologies/2024/1/gsn#optional">
        <rdfs:domain rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Statement"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <coreOrExtension xml:lang="en">Argument Pattern Extension</coreOrExtension>
        <renderedAs xml:lang="en">hollow ball</renderedAs>
        <rdfs:label xml:lang="en">optional</rdfs:label>
        <skos:definition xml:lang="en">Optional instantiation means that the relationship and the argument below may or may not be instantiated.</skos:definition>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/momcilovic/ontologies/2024/1/gsn#public -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/momcilovic/ontologies/2024/1/gsn#public">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/momcilovic/ontologies/2024/1/gsn#Assumption"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/momcilovic/ontologies/2024/1/gsn#Context"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/momcilovic/ontologies/2024/1/gsn#Goal"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/momcilovic/ontologies/2024/1/gsn#Justification"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/momcilovic/ontologies/2024/1/gsn#Solution"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <coreOrExtension xml:lang="en">Modular Extension</coreOrExtension>
        <renderedAs>miniature module symbol superimposed on an element at the top right</renderedAs>
        <rdfs:label xml:lang="en">public</rdfs:label>
        <skos:definition xml:lang="en">This indicates that the element is publicly visible in one or more interfaces of the module and can be referenced as an away element.</skos:definition>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/momcilovic/ontologies/2024/1/gsn#published -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/momcilovic/ontologies/2024/1/gsn#published">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/momcilovic/ontologies/2024/1/gsn#Template"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <coreOrExtension xml:lang="en">Argument Pattern Extension</coreOrExtension>
        <rdfs:label xml:lang="en">published</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/momcilovic/ontologies/2024/1/gsn#relationshipType -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/momcilovic/ontologies/2024/1/gsn#relationshipType">
        <rdfs:domain rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Statement"/>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:oneOf>
                    <rdf:Description>
                        <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                        <rdf:first>evidential</rdf:first>
                        <rdf:rest>
                            <rdf:Description>
                                <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
